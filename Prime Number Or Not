#include<iostream>
#include<string>
using namespace std;
string isPrime(int num){
    for(int i=2; i*i<=num; i++){
        if(num % i == 0){
            return "Not a Prime Number";
        }
    }
    return "Prime Number";
}
int main(){
    int num;
    cin>>num;
    cout<<isPrime(num);
    return 0;
}


******************************************************
OUTPUT:- 2
Prime Number
