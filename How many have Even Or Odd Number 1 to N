//to print t to n have how many number is even
#include<iostream>
using namespace std;
int main(){
    int num;
    cout<<"Enter the number: ";
    cin>>num;
    int evenCount = 0, oddCount = 0;
    for(int i=1; i<= num; i++){
        if(i % 2 == 0){
            evenCount++;
        }
        if( i%2 != 0){
            oddCount++;
        }
    }
    
    cout<<"Even Count: "<<evenCount<<endl;
    cout<<"Odd Count: "<<oddCount;
}
