//Sum Of Array 
#include<iostream>
using namespace std;
void sumOfArray(int arr[], int n){
    int sum = 0;
    for(int i=0; i<n; i++){
        sum += arr[i];
    }
    cout<<sum;
}
int main(){
    int arr[] = {5,3,2,6,8,9,0,2,4};
    int n = sizeof(arr)/sizeof(arr[0]);
    sumOfArray(arr, n);
    return 0;
}
