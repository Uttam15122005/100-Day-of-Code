// to print 1 to N all Even Number Sum
#include<iostream>
using namespace std;
int main(){
    int num;
    cin>>num;
    int sum = 0;
    for(int i=1; i<=num; i++){
        if(i % 2 == 0){
            sum = sum + i;
        }
    }
    cout<<"Sum of 1 to "<<num <<" total Even Number: "<<sum;
    return 0;
}
